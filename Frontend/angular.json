{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-edusharing": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "baseHref": "/edu-sharing/",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets/",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor/min",
                "output": "/assets/monaco/min/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/tinymce",
                "output": "/tinymce/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/tinymce-i18n/langs5",
                "output": "/tinymce/langs/"
              },
              { "glob": "**/*", "input": "node_modules/ng2-pdfjs-viewer/pdfjs", "output": "/assets/pdfjs" },
              { "glob": "**/*", "input": "node_modules/ngx-rendering-service-lib/assets", "output": "/" }
            ],
            "styles": [
              "src/app/core-ui-module/styles/core-base.scss",
              "src/styles/main.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "src/assets/js/nouislider.min.js",
              "node_modules/moment/min/moment-with-locales.min.js",
              "node_modules/chart.js/dist/chart.umd.js",
              "node_modules/tinymce/tinymce.min.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": [
              "qrcode",
              "uuid",
              "lodash",
              "moment"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "15kb",
                  "maximumError": "100kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "src/proxy.conf.js",
            "buildTarget": "angular-edusharing:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "angular-edusharing:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "angular-edusharing:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "src/assets/js/nouislider.min.js",
              "node_modules/moment/min/moment-with-locales.min.js",
              "node_modules/chart.js/dist/chart.umd.js",
              "node_modules/tinymce/tinymce.min.js"
            ],
            "styles": [
              "src/app/core-ui-module/styles/core-base.scss",
              "src/styles/main.scss"
            ],
            "assets": [
              "src/assets"
            ],
            "karmaConfig": "karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ],
            "cache": true
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": ".storybook",
            "browserTarget": "angular-edusharing:build",
            "compodoc": false,
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": ".storybook",
            "browserTarget": "angular-edusharing:build",
            "compodoc": false,
            "outputDir": "dist/storybook/angular-edusharing"
          }
        }
      }
    },
    "edu-sharing-api": {
      "projectType": "library",
      "root": "projects/edu-sharing-api",
      "sourceRoot": "projects/edu-sharing-api/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/edu-sharing-api/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/edu-sharing-api/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/edu-sharing-api/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/edu-sharing-api/tsconfig.spec.json",
            "karmaConfig": "projects/edu-sharing-api/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/edu-sharing-api/**/*.ts",
              "projects/edu-sharing-api/**/*.html"
            ],
            "cache": true
          }
        }
      }
    },
    "embed": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/embed",
      "sourceRoot": "projects/embed/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/embed",
            "index": "projects/embed/src/index.html",
            "main": "projects/embed/src/main.ts",
            "polyfills": "projects/embed/src/polyfills.ts",
            "tsConfig": "projects/embed/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/embed/src/favicon.ico",
              "projects/embed/src/assets"
            ],
            "styles": [
              "projects/embed/src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/embed/src/environments/environment.ts",
                  "with": "projects/embed/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "embed:build:production"
            },
            "development": {
              "buildTarget": "embed:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "embed:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": "projects/embed/src/polyfills.ts",
            "tsConfig": "projects/embed/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/embed/src/favicon.ico",
              "projects/embed/src/assets"
            ],
            "styles": [
              "projects/embed/src/styles.scss"
            ],
            "scripts": [],
            "karmaConfig": "projects/embed/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/embed/**/*.ts",
              "projects/embed/**/*.html"
            ]
          }
        }
      }
    },
    "app-as-web-component": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/app-as-web-component",
      "sourceRoot": "projects/app-as-web-component/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "deployUrl": "vendor/edu-sharing/",
            "outputHashing": "none",
            "outputPath": "dist/app-as-web-component",
            "index": "projects/app-as-web-component/src/index.html",
            "main": "projects/app-as-web-component/src/main.ts",
            "polyfills": "projects/app-as-web-component/src/polyfills.ts",
            "tsConfig": "projects/app-as-web-component/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets/",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor/min",
                "output": "/assets/monaco/min/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/tinymce",
                "output": "/tinymce/"
              },
              {
                "glob": "**/*",
                "input": "node_modules/tinymce-i18n/langs5",
                "output": "/tinymce/langs/"
              },
              "projects/app-as-web-component/src/favicon.ico",
              "projects/app-as-web-component/src/assets"
            ],
            "styles": [
              "src/app/core-ui-module/styles/core-base.scss",
              "src/styles/main.scss",
              "projects/app-as-web-component/src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "src/assets/js/nouislider.min.js",
              "node_modules/moment/min/moment-with-locales.min.js",
              "node_modules/chart.js/dist/chart.umd.js",
              "node_modules/tinymce/tinymce.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "15kb",
                  "maximumError": "100kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/app-as-web-component/src/environments/environment.ts",
                  "with": "projects/app-as-web-component/src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "src/proxy.conf.js"
          },
          "configurations": {
            "production": {
              "buildTarget": "app-as-web-component:build:production"
            },
            "development": {
              "buildTarget": "app-as-web-component:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "app-as-web-component:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": "projects/app-as-web-component/src/polyfills.ts",
            "tsConfig": "projects/app-as-web-component/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-as-web-component/src/favicon.ico",
              "projects/app-as-web-component/src/assets"
            ],
            "styles": [
              "projects/app-as-web-component/src/styles.scss"
            ],
            "scripts": [],
            "karmaConfig": "projects/app-as-web-component/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/app-as-web-component/**/*.ts",
              "projects/app-as-web-component/**/*.html"
            ]
          }
        }
      }
    },
    "edu-sharing-ui": {
      "projectType": "library",
      "root": "projects/edu-sharing-ui",
      "sourceRoot": "projects/edu-sharing-ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/edu-sharing-ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/edu-sharing-ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/edu-sharing-ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/edu-sharing-ui/tsconfig.spec.json",
            "karmaConfig": "projects/edu-sharing-ui/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/edu-sharing-ui/**/*.ts",
              "projects/edu-sharing-ui/**/*.html"
            ]
          }
        }
      }
    },
    "rendering-service": {
      "projectType": "library",
      "root": "projects/rendering-service-web-component",
      "sourceRoot": "projects/rendering-service-web-component/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/rendering-service-web-component/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/rendering-service-web-component/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/rendering-service-web-component/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/rendering-service-web-component/tsconfig.spec.json",
            "karmaConfig": "projects/rendering-service-web-component/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/rendering-service-web-component/**/*.ts",
              "projects/rendering-service-web-component/**/*.html"
            ],
            "cache": true
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "es",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "es"
    },
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ],
    "analytics": false
  }
}
