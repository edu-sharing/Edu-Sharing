    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "docs": "typedoc --mode file --experimentalDecorators --excludePrivate --ignoreCompilerErrors --target 'ES6' --module 'commonjs' --out docs src",
    "print-licenses": "node licenses.js",
    "gitlog": "..\\Edu-Sharing-Internal\\Frontend\\gitlog.sh",
    "start-storybook": "start-storybook -p 4300 -s ./src/assets",
