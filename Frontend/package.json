{
  "name": "angular-edusharing",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "angular-cli": {},
  "scripts": {
    "prestart": "npm run build:edu-sharing-api:dev  && npm run build:edu-sharing-ui",
    "start": "ng serve",
    "start-public": "npm run prestart && ng serve --host 0.0.0.0 --disable-host-check",
    "serve-dist": "node scripts/server.js",
    "build:edu-sharing-api": "(cd projects/edu-sharing-api && npm run update-if-needed) && node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build edu-sharing-api",
    "build:edu-sharing-ui": "node --max_old_space_size=2048 ./node_modules/@angular/cli/bin/ng build edu-sharing-ui",
    "build:edu-sharing-api:dev": "(cd projects/edu-sharing-api && npm run update-if-needed) && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build edu-sharing-api --configuration development",
    "publish:edu-sharing-api": "npm run build:edu-sharing-api && cd dist/edu-sharing-api && npm publish",
    "prebuild": "npm run build:edu-sharing-api && npm run build:edu-sharing-ui",
    "build": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --configuration production --delete-output-path false --base-href /edu-sharing/",
    "copy-build-to-kubernetes": "kubectl  --kubeconfig=${npm_config_kubeconfig} --namespace=${npm_config_namespace} cp ./dist/. edusharing-repository-service-0:/opt/bitnami/tomcat/temp/0-edu-sharing/",
    "copy-build-to-docker": "docker container cp ./dist/. $(docker ps --filter name=^repository-service --quiet):/opt/bitnami/tomcat/temp/0-edu-sharing/",
    "build-and-copy-to-docker": "npm run build && npm run copy-build-to-docker",
    "prebuild-dev": "npm run build:edu-sharing-api:dev && npm run build:edu-sharing-ui",
    "build-dev": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --delete-output-path false --base-href /edu-sharing/ --configuration=development",
    "check:lint": "ng lint --max-warnings=835",
    "fix:lint": "ng lint --fix",
    "check:format": "prettier --check src projects/*/src",
    "fix:format": "prettier --write src projects/*/src",
    "test": "ng test --no-watch",
    "test-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI --reporters=verbose",
    "pree2e": "rm -rf playwright/out && tsc --incremental -p tests/tsconfig.json",
    "e2e": "playwright test",
    "e2e-watch-compile": "rm -rf playwright/out && tsc --watch -p tests/tsconfig.json",
    "e2e-prepare-edu-sharing": "rm -f playwright/storage/*.json && ts-node --esm tests/scripts/prepare-edu-sharing.mts",
    "test:edu-sharing-frontend": "ng test angular-edusharing",
    "test:edu-sharing-api": "ng test edu-sharing-api",
    "test:edu-sharing-ui": "ng test edu-sharing-ui",
    "docs": "typedoc --mode file --experimentalDecorators --excludePrivate --ignoreCompilerErrors --target 'ES6' --module 'commonjs' --out docs src",
    "print-licenses": "node licenses.js",
    "storybook": "BROWSER=true ng run angular-edusharing:storybook",
    "build-storybook": "ng run angular-edusharing:build-storybook",
    "prepare": "cd .. && husky install Frontend/.husky && cd Frontend/src/app/core-module && git config core.hooksPath ../../../.husky || true"
  },
  "private": false,
  "subPackages": [
    "src/app/extension"
  ],
  "dependencies": {
    "@angular-slider/ngx-slider": "^18.0.0",
    "@angular/animations": "^18.2.12",
    "@angular/cdk": "^18.2.12",
    "@angular/common": "^18.2.12",
    "@angular/compiler": "^18.2.12",
    "@angular/core": "^18.2.12",
    "@angular/elements": "^18.2.12",
    "@angular/forms": "^18.2.12",
    "@angular/material": "^18.2.12",
    "@angular/platform-browser": "^18.2.12",
    "@angular/platform-browser-dynamic": "^18.2.12",
    "@angular/router": "^18.2.12",
    "@angular/youtube-player": "^18.0.3",
    "@ngx-translate/core": "^14.0.0",
    "@tinymce/tinymce-angular": "^7.0.0",
    "tinymce": "^7.6.0",
    "tinymce-i18n": "^23.9.11",
    "angular-material-css-vars": "^7.0.0",
    "angular-resizable-element": "^7.0.2",
    "angularx-qrcode": "^18.0.2",
    "chart.js": "^4.4.2",
    "core-js": "^3.6.4",
    "edu-sharing-extension-dependencies": "file:src/app/extension",
    "jquery": "^3.7.1",
    "jsonpath": "^1.1.1",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.4",
    "ng-lazyload-image": "^9.1.3",
    "ng2-pdfjs-viewer": "^18.0.0",
    "ngx-monaco-editor-v2": "^18.1.0",
    "ngx-rendering-service-api": "^0.0.22",
    "ngx-rendering-service-lib": "^0.0.45",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.1",
    "uuid": "^3.3.2",
    "webpack": "^5.75.0",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.3",
    "@angular-eslint/builder": "~18.0.1",
    "@angular-eslint/eslint-plugin": "~18.0.1",
    "@angular-eslint/eslint-plugin-template": "~18.0.1",
    "@angular-eslint/schematics": "~18.0.1",
    "@angular-eslint/template-parser": "~18.0.1",
    "@angular/cli": "^18.0.3",
    "@angular/compiler-cli": "^18.2.12",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@playwright/test": "^1.29.2",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/angular": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/testing-library": "^0.2.2",
    "@types/core-js": "^2.5.2",
    "@types/jasmine": "~5.1.1",
    "@types/jasminewd2": "~2.0.12",
    "@types/jest": "^29.1.2",
    "@types/jquery": "^3.5.9",
    "@types/lodash": "^4.14.182",
    "@types/node": "^20.4.2",
    "@types/qrcode": "^1.5.2",
    "@types/quill": "^1.3.10",
    "@types/tough-cookie": "^4.0.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "babel-jest": "^29.1.2",
    "babel-loader": "^8.0.6",
    "codelyzer": "^6.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.39.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-storybook": "^0.6.15",
    "got": "^12.5.2",
    "http-server": "^14.1.1",
    "husky": "^7.0.4",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "^29.1.2",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-verbose-reporter": "~0.0.8",
    "license-checker": "^25.0.1",
    "lint-staged": "^12.1.7",
    "ng-packagr": "^18.0.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.1.6",
    "tough-cookie": "^4.1.2",
    "ts-node": "~10.9.1",
    "typedoc": "^0.16.9",
    "typescript": "~5.4.5"
  },
  "lint-staged": {
    "*.{js,css,md,ts,html,scss}": "prettier --write"
  }
}
